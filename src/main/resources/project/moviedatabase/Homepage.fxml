<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.moviedatabase.HomepageController">
   <children>
      <Label fx:id="prodCompLabel" alignment="CENTER" layoutX="17.0" layoutY="48.0" prefHeight="88.0" prefWidth="607.0" text="Label" textAlignment="CENTER">
         <font>
            <Font name="Copperplate Gothic Light" size="38.0" />
         </font>
      </Label>
      <Button fx:id="buttonRecentMovies" layoutX="194.0" layoutY="264.0" mnemonicParsing="false" onAction="#onRecentMoviesClick" prefHeight="50.0" prefWidth="258.0" text="Most Recent Movies">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font></Button>
      <Button fx:id="buttonMaxRev" alignment="CENTER" layoutX="134.0" layoutY="330.0" mnemonicParsing="false" onAction="#onMaxRevClick" text="Movies with Maximum Revenue">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
      <Button fx:id="buttonTotalProfit" alignment="CENTER" layoutX="248.0" layoutY="399.0" mnemonicParsing="false" onAction="#onTotalProfitClick" text="Total Profit">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
      <Button fx:id="buttonLogOut" alignment="CENTER" layoutX="260.0" layoutY="606.0" mnemonicParsing="false" onAction="#onLogOutClick" text="Log Out">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
      <Button fx:id="buttonAddMovie" alignment="CENTER" layoutX="214.0" layoutY="468.0" mnemonicParsing="false" onAction="#onAddMovieClick" text="Add New Movie">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
      <Button fx:id="buttonTransferMovie" alignment="CENTER" layoutX="223.0" layoutY="540.0" mnemonicParsing="false" onAction="#onTransferMovieClick" text="Transfer Movie">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
      <Button fx:id="buttonSearchMovie" alignment="CENTER" layoutX="226.0" layoutY="200.0" mnemonicParsing="false" onAction="#onSearchMovieClick" text="Search a Movie">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
      <Button fx:id="buttonMovieList" layoutX="233.0" layoutY="136.0" mnemonicParsing="false" onAction="#onMovieListClick" text="List of Movies">
         <font>
            <Font name="Palatino Linotype" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
