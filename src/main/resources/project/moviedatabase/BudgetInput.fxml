<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="772.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.moviedatabase.BudgetInputController">
    <children>
        <Label fx:id="headerLabel1" alignment="CENTER" layoutX="23.0" layoutY="97.0" prefHeight="35.0" prefWidth="397.0" text="Enter minimum budget :">
            <font>
                <Font name="Palatino Linotype" size="27.0" />
            </font>
        </Label>
        <Button fx:id="buttonGoBack" layoutX="206.0" layoutY="274.0" mnemonicParsing="false" onAction="#onGoBackClick" text="Go Back">
            <font>
                <Font name="Palatino Linotype" size="24.0" />
            </font>
        </Button>
        <TextField fx:id="inputTextFieldLow" layoutX="435.0" layoutY="91.0" prefHeight="47.0" prefWidth="274.0" promptText="Please enter an integer">
            <font>
                <Font name="Palatino Linotype" size="24.0" />
            </font>
        </TextField>
        <Button fx:id="buttonConfirm" alignment="CENTER" layoutX="420.0" layoutY="274.0" mnemonicParsing="false" onAction="#onConfirmClick" text="Confirm">
            <font>
                <Font name="Palatino Linotype" size="24.0" />
            </font>
        </Button>
        <Label fx:id="headerLabel2" alignment="CENTER" layoutX="23.0" layoutY="176.0" prefHeight="35.0" prefWidth="397.0" text="Enter maximum budget :">
            <font>
                <Font name="Palatino Linotype" size="27.0" />
            </font>
        </Label>
        <TextField fx:id="inputTextFieldHigh" layoutX="435.0" layoutY="170.0" prefHeight="47.0" prefWidth="274.0" promptText="Please enter an integer">
            <font>
                <Font name="Palatino Linotype" size="24.0" />
            </font>
        </TextField>
    </children>
</AnchorPane>
